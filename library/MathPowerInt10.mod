MODULE MathPowerInt10;

 VAR val*: ARRAY 10 OF INTEGER;

BEGIN
 val[0] := 1;
 val[1] := 10;
 val[2] := 100;
 val[3] := 1000;
 val[4] := 10000;
 val[5] := 100000;
 val[6] := 1000000;
 val[7] := 10000000;
 val[8] := 100000000;
 val[9] := 1000000000
END MathPowerInt10.
